{
  "private": true,
  "name": "rankist",
  "productName": "rankist",
  "description": "the best place to found and create ranking lists",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "start": "poi --port 80",
    "build": "poi build",
    "report": "poi build --bundle-report",
    "serve": "serve dist --single",
    "test": "cross-env NODE_ENV=test ava",
    "semantic-release": "[[ $(git rev-parse --abbrev-ref HEAD) == \"master\" ]] && semantic-release pre && semantic-release post"
  },
  "babel": {
    "presets": [
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "author": {
    "name": "albizures",
    "email": "albizures3601@gmail.com"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup.js"
    ]
  },
  "dependencies": {
    "chai": "^4.1.2",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "offline-plugin": "^4.8.0",
    "promise-polyfill": "^6.0.2",
    "vuetify": "^0.15.2"
  },
  "devDependencies": {
    "poi": "^9.0.0",
    "poi-preset-bundle-report": "^2.0.0",
    "serve": "^3.1.0",
    "ava": "^0.19.1",
    "cross-env": "^5.0.0",
    "browser-env": "^2.0.31",
    "require-extension-hooks": "^0.2.0",
    "require-extension-hooks-vue": "^0.3.1",
    "require-extension-hooks-babel": "^0.1.1",
    "poi-preset-offline": "^9.0.0",
    "semantic-release": "^7.0.2"
